# Example environment file for Netlify deployment
# Copy this to .env and fill in your values

# MongoDB Connection (for local development - not needed for Netlify frontend)
DATABASE_URL=mongodb+srv://username:password@cluster.mongodb.net/database

# GitHub OAuth (for local development - not needed for Netlify frontend)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Session Secret (for local development - not needed for Netlify frontend)
SESSION_SECRET=your_session_secret

# API URL for Frontend (Important for Netlify)
# This should point to your backend API endpoint
VITE_API_URL=https://your-backend-api.com

# Admin Configuration
ADMIN_USERS=["user1","user2"]